! Apply changes: xrdb ~/.Xdefaults

xterm*metaSendsEscape: true
xterm*directColor: true
xterm*allowTcapOps: true
xterm*disallowedTcapOps:
xterm*decGraphicsID: 340
xterm*decTerminalID: 420
xterm*numColorRegisters: 256
xterm*sixelScrolling: true
xterm*faceName: RobotoMono Nerd Font Mono
xterm*faceSize: 14
xterm*saveLines: 4096
xterm*scrollBar: false
xterm*rightScrollBar: false
xterm*scrollTtyOutput: false
xterm*highlightSelection: true
xterm*trimSelection: true
xterm*selectToClipboard: true
xterm*ptyInitialErase: true
xterm*backarrowKeyIsErase: true

XClock.Clock.analog: false
XClock.Clock.face: RobotoMono Nerd Font Mono:size=36

XCalc.geometry: 600x800
XCalc*Font: -*-roboto-*-r-*-*-30-*-*-*-*-*-*-ru

! Disable Perl extension
! If you do not use the Perl extension features, you can improve the security
! and speed by disabling Perl extensions completely.
URxvt.perl-ext:
URxvt.perl-ext-common: clipboard,keyboard-select,selection-to-clipboard,fullscreen,vtwheel

!URxvt.termName: xterm-256color
URxvt.termName: rxvt-unicode-256color
URxvt.font:           xft:RobotoMono Nerd Font Mono:size=14:style=Regular:antialias=true:autohint=true,\
                      xft:Liberation Mono:size=14,\
                      xft:unifont,\
                      xft:Font Awesome 6 Free:style=Solid
URxvt.boldFont:       xft:RobotoMono Nerd Font Mono:size=14:style=Medium
URxvt.italicFont:     xft:RobotoMono Nerd Font Mono:size=14:style=Italic
URxvt.boldItalicFont: xft:RobotoMono Nerd Font Mono:size=14:style=Medium Italic
URxvt.transparent: false
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.geometry: 140x40
URxvt.cursorBlink: false
URxvt.cursorUnderline: false
URxvt.saveline: 8192
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollstyle: plain
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.iso14755: false
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true
URxvt.secondaryScroll:   false
URxvt.secondaryWheel:    true
URxvt.secondaryScreen:   true
URxvt.mouseWheelScrollPage: false
URxvt.inheritPixmap:    true
URxvt.reverseVideo:     false
URxvt.skipBuiltinGlyphs: true
URxvt.borderWidth: 0
URxvt.internalBorder: 3
URxvt.externalBorder: 0

URxvt.copyCommand:  xclip -i -sel c -f | xclip -i -sel p
URxvt.pasteCommand: xclip -o -sel c

URxvt.urlLauncher: opera
URxvt.underlineURLs: true
URxvt.urlButton: 1

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up:      \033[1;2A
