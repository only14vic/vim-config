URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.M-Down:        \033[1;3B
URxvt.keysym.M-Up:          \033[1;3A
URxvt.keysym.M-Right:       \033[1;3C
URxvt.keysym.M-Left:        \033[1;3D
URxvt.keysym.Home:          \033[7~
URxvt.keysym.End:           \033[8~
URxvt.keysym.Delete:        \033[3~
URxvt.keysym.Control-Delete:\033[3;5~
URxvt.keysym.PageUp:        \033[5~^
URxvt.keysym.PageDown:      \033[6~^
URxvt.keysym.F1:            \033OP
URxvt.keysym.F2:            \033OQ
URxvt.keysym.F3:            \033OR
